    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <style>
        @media (orientation: landscape) {
            .landscape-cols {
                column-count: 2;
            }
        }

        @media (orientation: portrait) {
            .portrait-cols {
                column-count: 1;
            }
        }

            /* Estilos para los enlaces con ID "hover" al hacer hover */
        a#hover {
            position: relative; /* Para permitir el ajuste del z-index */
            transition: transform 2s, color 2s, z-index 2s; /* Transición de 2 segundos */
        }

        a#hover:hover {
            transform: scale(1.8); /* Disminuye el tamaño en un 80% */
            color: orange; /* Cambia el color a anaranjado */
            z-index: 1; /* Coloca el elemento en la parte superior */
        }
    </style>

    <section class="soft py-5 d-flex align-items-center" >
        <div class="container text-light" style="margin-top: -0.5rem; width: 95% !important;">
