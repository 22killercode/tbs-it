    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <style>
    @media (orientation: landscape) {
    .landscape-cols {
    column-count: 2;
    }
    }

    @media (orientation: portrait) {
    .portrait-cols {
    column-count: 1;
    }
    }

    /* Estilos para los enlaces con ID "hover" al hacer hover */
    a#hover {
    position: relative; /* Para permitir el ajuste del z-index */
    transition: transform 2s, color 2s, z-index 2s; /* Transición de 2 segundos */
    }

    a#hover:hover {
    transform: scale(1.2); /* Disminuye el tamaño en un 80% */
    color: orange; /* Cambia el color a anaranjado */
    z-index: 1; /* Coloca el elemento en la parte superior */
    }
    /* Estilo por defecto para dispositivos de escritorio */
    .imge {
    max-width: 100%;
    height: auto;
    display: block; /* Asegura que las imágenes no tengan espacios adicionales por debajo */
    margin-bottom: 10px; /* Espacio entre las imágenes en dispositivos de escritorio */
    }

    /* Media query para dispositivos con un ancho máximo de 767px (pantallas de celular) */
    @media (max-width: 767px) {
    .imge {
    width: 100%; /* Hace que las imágenes ocupen el ancho completo en dispositivos móviles */
    margin-bottom: 20px; /* Espacio mayor entre las imágenes en dispositivos móviles */
    }
    }
    </style>
